{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm ci",
  "framework": "vite",
  "redirects": [
    {
      "source": "/",
      "destination": "/blazemetrics/",
      "statusCode": 302
    }
  ],
  "rewrites": [
  {
  "source": "/blazemetrics/(.*)",
  "destination": "/index.html"
  }
  ],
  "headers": [
  {
  "source": "/blazemetrics/assets/(.*)",
  "headers": [
  {
  "key": "Cache-Control",
  "value": "public, max-age=31536000, immutable"
  }
  ]
  }
  ]
}

